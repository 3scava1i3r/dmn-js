{
  "name": "dmn-js",
  "description": "A dmn toolkit and web modeler",
  "version": "5.0.0-0",
  "scripts": {
    "test": "karma start",
    "dev": "npm run test -- --no-single-run --auto-watch",
    "test:distro": "node tasks/test-distro.js",
    "distro": "node tasks/build-distro.js",
    "prepublishOnly": "run-s build distro test:distro",
    "build": "del lib && babel -s -q -d lib src"
  },
  "main": "./lib/index.js",
  "author": {
    "name": "Sebastian Stamm",
    "url": "https://github.com/SebastianStamm"
  },
  "contributors": [
    {
      "name": "bpmn.io contributors",
      "url": "https://github.com/bpmn-io"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bpmn-io/dmn-js"
  },
  "license": "SEE LICENSE IN LICENSE",
  "keywords": [
    "dmn",
    "dmn-js",
    "toolkit",
    "web modeler",
    "modeler",
    "modeling",
    "decision modeling"
  ],
  "devDependencies": {
    "cpx": "^1.5.0",
    "del": "^3.0.0",
    "diagram-js": "^2.4.0",
    "dmn-font": "^0.4.1",
    "execa": "^0.9.0",
    "min-dom": "^3.0.0",
    "mkdirp": "^0.5.1",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-license": "^0.6.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "dependencies": {
    "dmn-js-decision-table": "^5.0.0-0",
    "dmn-js-drd": "^5.0.0-0",
    "dmn-js-literal-expression": "^5.0.0-0",
    "dmn-js-shared": "^5.0.0-0"
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
