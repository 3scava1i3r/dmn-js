{
  "name": "dmn-js",
  "version": "1.0.0",
  "description": "A dmn toolkit and web modeler",
  "scripts": {
    "dev": "karma start test/config/karma.unit.js --log-level debug",
    "test": "karma start test/config/karma.unit.js --single-run --no-auto-watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bpmn-io/dmn-js"
  },
  "keywords": [
    "dmn",
    "dmn-js",
    "toolkit",
    "web modeler",
    "modeler",
    "modeling",
    "decision modeling"
  ],
  "author": {
    "name": "Sebastian Stamm",
    "url": "https://github.com/SebastianStamm"
  },
  "contributors": [
    {
      "name": "bpmn.io contributors",
      "url": "https://github.com/bpmn-io"
    }
  ],
  "license": "SEE LICENSE IN LICENSE",
  "browserify": {
    "transform": [
      "babelify",
      [
        "stringify",
        {
          "global": true,
          "extensions": [
            ".dmn",
            ".html",
            ".css"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-inferno": "^3.3.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babelify": "^7.3.0",
    "browserify": "^13.0.0",
    "browserify-derequire": "^0.9.1",
    "bundle-collapser": "^1.1.1",
    "chai": "~3.5.0",
    "eslint": "^2.11.1",
    "eslint-plugin-mocha": "^3.0.0",
    "jsondiffpatch": "^0.1.26",
    "karma": "^1.7.1",
    "karma-browserify": "^5.0.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "~1.2.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-sinon-chai": "^1.3.2",
    "karma-spec-reporter": "0.0.26",
    "mocha": "^4.0.0",
    "mocha-test-container-support": "0.2.0",
    "puppeteer": "^0.11.0",
    "sinon": "^2.4.1",
    "sinon-chai": "^2.14.0",
    "source-map-concat": "^1.0.0",
    "uglify-js": "^3.1.5",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.5",
    "diagram-js": "^0.21.1",
    "diagram-js-direct-editing": "^0.19.0",
    "dmn-moddle": "^0.6.0",
    "ids": "^0.2.0",
    "inherits": "^2.0.1",
    "lodash": "^3.0.1",
    "min-dom": "^0.2.0",
    "object-refs": "^0.1.0",
    "selection-update": "^0.1.2",
    "stringify": "^5.1.0",
    "table-js": "^0.6.4",
    "tiny-svg": "^0.1.1"
  }
}
